{
    "name": "ripaclub/imgman",
    "description": "A library designed to create, modify, handle, and store images from any protocol",
    "homepage": "https://github.com/ripaclub/imgman",
    "license": "BSD-2-Clause",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-servicemanager": "~2.5.1",
        "zendframework/zend-http": "~2.5.1",
        "zendframework/zend-stdlib": "~2.5.1"
    },
    "require-dev": {
        "aws/aws-sdk-php": "2.8.29",
        "phpunit/phpunit": "~4.2",
        "zendframework/zend-mvc": "~2.5",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggested": {
        "aws/aws-sdk-php": "To store image in S3 storage and recover them from CloudFront"
    },
    "authors": [
        {
            "name": "Antonio Visalli",
            "email": "antonino.visalli@gmail.com",
            "homepage": "http://github.com/visa4"
        },
        {
            "name": "Leonardo Di Donato",
            "email": "leodidonato@gmail.com",
            "homepage": "http://github.com/leodido"
        },
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com",
            "homepage": "http://github.com/leogr"
        }
    ],
    "autoload": {
        "psr-4": {
            "ImgMan\\": "library",
            "ImgManTest\\": "tests"
        }
    },
    "keywords": [
        "image manager",
        "image",
        "picture",
        "image magick",
        "gd",
        "imagick",
        "renditions",
        "resize",
        "crop",
        "rotate",
        "fit in",
        "fit out",
        "images",
        "pictures",
        "figures",
        "figure",
        "streams"
    ]
